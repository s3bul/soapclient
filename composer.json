{
    "name": "s3bul/soapclient",
    "description": "Extension for PHP SoapClient class",
    "version": "1.1.0",
    "keywords": [
        "soapclient",
        "php"
    ],
    "minimum-stability": "stable",
    "homepage": "https://github.com/s3bul/soapclient",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sebastian Korzeniecki",
            "email": "seba5zer@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0",
        "ext-soap": "*",
        "ext-simplexml": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "S3bul\\": "src/"
        }
    },
    "scripts": {
        "safe-update": [
            "composer update --no-interaction --no-ansi --ignore-platform-reqs"
        ]
    }
}
